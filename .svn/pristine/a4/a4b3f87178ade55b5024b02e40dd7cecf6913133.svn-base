  dateString(dat){
    let date = new Date(dat);
    let month = date.getMonth() + 1;
    let mm = month < 10 ? '0' + month : month.toString();
    let day  = date.getDate();
    let dd = day < 10 ? '0' + day : day.toString();
    return date.getFullYear()+ '-' + mm + '-' + dd ;
  };

