@media screen {

    
        .TableToolTipCalendar .table {
          border: 0;
          vertical-align: middle;
        }

        .TableToolTipCalendar .table caption {
          font-size: 1em;
        }

        .TableToolTipCalendar thead{
          display: none;
        }

        .TableToolTipCalendar  .table tr {
          //border-bottom: 5px solid #ddd;
          display: block;
        }

        .TableToolTipCalendar  .table td { 
          display: block;
          font-size: 13px;
          //font-family: calibri;
          text-align: left;
          height: 100%;
          padding-left: 10px;
          
          padding-right: 10px;
          //border-top: none !important;
         // border-color: gray;
        }
      
        #TableToolTipCalendar [name=EventTable]{
          display: flex;
          flex-direction: column-reverse;
        }

        .TableToolTipCalendar td{
            min-height: 20px;
        }

        .TableToolTipCalendar tfoot td{
            min-height: 20px;
        }

        .TableToolTipCalendar .table td:before {

           content: attr(data-label);
           float: left;
           text-transform:capitalize;
           font-weight: normal;
           color: #05a2c6 !important;
           font-size: 13px;
           padding-right: 5px;
           width: 110px;
           min-height: 20px;
           height: auto;
          
        }

        .TableToolTipCalendar  .table td:last-child {
          border-bottom: 0;
        }

        

        .TableToolTipCalendar td:first-of-type, thead:first-of-type, tfoot:first-of-type {  
          margin-bottom: 0px;
        }

        

      //   .TableToolTipCalendar td{
      //     min-height: 20px;
      // }
}

.CalendarEventTile{        
  font-size: 80%;        
  font-weight: 500;
  padding:2px;        
  border-bottom: solid 1px #d0d0d0;        
  word-wrap:break-word;
  word-break: break-word; 
}
@media screen and (max-width: 960px) {
  .difftab .mat-table {
    border: 0;
    vertical-align: middle;
  }

  .difftab .mat-table caption {
    font-size: 1em;
  }


.difftab .mat-header-row{
      display: none;
  }
.difftab  .mat-table .mat-row {
    border-bottom: 5px solid #ddd;
    display: block;
  }
  /*
  .mat-table .mat-row:nth-child(even) {background: #CCC}
  .mat-table .mat-row:nth-child(odd) {background: #FFF}
  */
  .difftab  .mat-table .mat-cell {
    border-bottom: 1px solid #ddd;
    display: block;
    font-size: 12px;
    text-align: right;
  //   font-weight: bold;
    //height: 20px;
    height: auto;;
    margin-bottom: 2%;
    padding-left: 10px;
    padding-right: 10px;
  }

  .difftab .mat-cell{
      min-height: 30px;        
      max-width: 100%;    
  }
  
  .difftab  .mat-footer-cell{
      min-height: 30px;
  }
  .difftab .mat-table .mat-cell:before {
    /*
    * aria-label has no advantage, it won't be read inside a table
    content: attr(aria-label);
    */
    content: attr(data-label);
    float: left;
    text-transform:capitalize;
    font-weight: normal;
    color: #05a2c6 !important;
    font-size: 12px;
  }
  .difftab  .mat-table .mat-cell:last-child {
    border-bottom: 0;
  }
  .difftab .mat-table .mat-cell:first-child {
    margin-top: 2%;
    min-height: 50px;
  }

  .difftab mat-cell:first-of-type, mat-header-cell:first-of-type, mat-footer-cell:first-of-type {
  padding-left: 10px !important; 
  }

  /*
  .CalendarEventTile{            
      text-align: left;
      margin: 0.5px 2px;
      font-size: 80%;
      color: black;
      font-weight: 700;
      padding: 2px;
      border: solid;
      border-width: thin;
      border-color: #abacb3;            
      overflow: hidden; white-space: nowrap; text-overflow: ellipsis; word-break: break-all; word-wrap: break-word;
  }   
  */

  .CalendarEventTile{            
      text-align: left;
      padding:5px;        
      border-bottom: solid 1px #d0d0d0;                                                   
      overflow: hidden; white-space: unset; text-overflow: ellipsis; word-break: break-word; word-wrap: break-word;
  }   
  
  

  .cal-month-view .cal-cell{
      width:100% !important;
      display: grid !important;
      table-layout: fixed !important;
  }

  // .cal-month-view .cal-day-cell {
  //     min-height: 250px;
  // }
}

